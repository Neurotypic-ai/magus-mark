{
  "name": "lbh-typescript-viewer",
  "version": "1.0.0",
  "description": "TypeScript codebase visualization tool",
  "main": "dist/index.js",
  "bin": {
    "typescript-viewer": "dist/bin/typescript-viewer.js"
  },
  "type": "module",
  "scripts": {
    "dev": "pnpm run --filter lbh-typescript-viewer --parallel \"/^dev:.*/\"",
    "dev:ui": "vite",
    "dev:server": "nodemon --watch src/db --exec 'tsx' src/server.ts",
    "build": "pnpm typecheck && vite build",
    "preview": "vite preview",
    "analyze": "node dist/bin/typescript-viewer.js analyze",
    "serve": "node dist/bin/typescript-viewer.js serve",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "visualization",
    "dependency-graph",
    "analysis"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dagrejs/dagre": "^1.1.4",
    "@duckdb/node-api": "1.2.2-alpha.18",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@xyflow/react": "^12.5.5",
    "@xyflow/system": "^0.0.55",
    "apache-arrow": "^19.0.1",
    "ast-types": "^0.16.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "elkjs": "^0.10.0",
    "jscodeshift": "^17.3.0",
    "mermaid": "^11.6.0",
    "ora": "^8.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "read-package-up": "^11.0.0",
    "read-pkg": "^9.0.1",
    "recast": "^0.23.11",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/dagre": "^0.7.52",
    "@types/jscodeshift": "^17.3.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "nodemon": "^3.1.9",
    "vite": "^6.2.5",
    "vite-bundle-analyzer": "^0.18.1",
    "vite-plugin-node-polyfills": "^0.23.0"
  }
}
