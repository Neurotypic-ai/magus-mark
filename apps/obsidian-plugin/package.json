{
  "author": "Kevin Hallmark <obsdian-magic@littleblackhat.com>",
  "dependencies": {
    "@codemirror/language": "^6.11.0",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.36.5",
    "@obsidian-magic/core": "workspace:*"
  },
  "description": "AI-powered tagging system for organizing AI chat history in Obsidian",
  "devDependencies": {
    "@axe-core/puppeteer": "^4.10.1",
    "@types/node": "^22.14.1",
    "@vitest/coverage-v8": "^3.1.1",
    "ejs": "^3.1.10",
    "electron": "^35.1.5",
    "obsidian": "^1.8.7",
    "puppeteer": "^24.6.1",
    "source-map": "^0.7.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "ai",
    "tagging",
    "chatgpt"
  ],
  "license": "MIT",
  "main": "dist/main.js",
  "name": "obsidian-magic",
  "peerDependencies": {
    "obsidian": "^1.8.7"
  },
  "private": true,
  "scripts": {
    "build": "pnpm run clean && tsc -b",
    "bundle-analysis": "node .github/scripts/analyze-bundle.js",
    "check-accessibility": "node .github/scripts/check-accessibility.js",
    "clean": "rm -rf dist",
    "dev": "tsc -b --watch",
    "format": "prettier --write 'src/**/*.{ts,js,json,css}' 'tests/**/*.{ts,js}'",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "package": "pnpm run build && mkdir -p release && cp -r dist manifest.json styles.css release/",
    "prebuild": "pnpm run lint && pnpm run typecheck",
    "prerelease": "pnpm run test && pnpm run check-accessibility",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "version": "0.1.0"
}
